<!--header-->
<%- include('includes/header') %> 
<div class="row">

<!--sidebar-->
<%- include('includes/sidebar') %>


<div class="col-lg-9">
    <h1 class="text-center">Add_Project_category</h1>
    <div class="row p-3">
    <div class="col-lg-2"></div>
    <div class="col-lg-8">
        <% if(msg!="") {%>
            <div class="alert alert-primary" role="alert">
                <strong><%= msg %></strong>
            </div>
            <% }%>
             
        
                <form action="/admin/add_project_category" method="post">
                  
                    <div class="form-group">
                        <label for="">Id</label>
                        <input type="text" required
                            class="form-control" name="id" id="" aria-describedby="helpId" placeholder="">
                        <small id="helpId" class="form-text text-muted">*</small>
                        </div>
                    <div class="form-group">
                    <label for="">Name</label>
                    <input type="text" required
                        class="form-control" name="name" id="" aria-describedby="helpId" placeholder="">
                    <small id="helpId" class="form-text text-muted">*</small>
                    </div>
        
                    <div class="form-group">
                    <label for="">Status</label>
                    <input type="status" required
                        class="form-control" name="status" id="" aria-describedby="helpId" placeholder="">
                    <small id="helpId" class="form-text text-muted">*</small>
                    </div>
                
                        <div class="form-group">
                            <label for="">Added_date</label>
                            <input type="date" required
                                class="form-control" name="added_date" id="" aria-describedby="helpId" placeholder="">
                            <small id="helpId" class="form-text text-muted">*</small>
                            </div>
                        <div class="form-group">
                            <label for="">Modify_date</label>
                            <input type="date" required
                                class="form-control" name="modify_date" id="" aria-describedby="helpId" placeholder="">
                            <small id="helpId" class="form-text text-muted">*</small>
                            </div>
                            
            
                        
                            <button type="submit" class="btn btn-primary">Submit</button>
                            <button type="submit" class="btn btn-primary" onclick="goBack()">Back</button>
            </form>
    </div>   
    </div>


</div>


</div>
<script>
    function add() {
      window.open("http://localhost:8000/admin/add_project_category/");
    }
   
  </script>

<!--footer-->
<%- include('includes/footer') %> 